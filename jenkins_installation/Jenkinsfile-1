@Library('shared-library') _
pipeline {
  agent {
    docker { image 'node:16-alpine' }
  }
  stages {
    stage('Test') {
      steps {
        sh 'node --version'
      }
    }
    stage('Random Number') {
            steps {
                script {
                    def utils = new my_playground.example.Utils()
                    echo "Random Number: ${utils.getRandomNumber()}"
                }
            }
        }
    }
}
